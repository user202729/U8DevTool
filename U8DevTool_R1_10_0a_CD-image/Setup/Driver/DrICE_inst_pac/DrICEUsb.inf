[Version] 
Signature = "$Windows NT$" 
Class=USB
ClassGUID={36fc9e60-c465-11cf-8056-444553540000}
Provider=%LAPISsemi% 
DriverVer=08/29/2012,3.00.0000
CatalogFile=driceusb.cat


; ================= Class section =====================
;[ClassInstall32]
;Addreg=USBClassReg 

;[ClassInstall32.ntamd64]
;Addreg=USBClassReg 

;[SampleClassReg]
;[USBClassReg]
;HKR,,,0,%ClassName%
;HKR,,Icon,,-5

[DestinationDirs] 
DrICEUsb.Files.Ext= 12

; ================= Device section =====================

[Manufacturer] 
%MfgName%=LAPISsemi,NTx86,NTamd64

; For NT
[LAPISsemi]
%USB\VID_070A&PID_401B%=DrICEUsb.Dev, USB\VID_070A&PID_401B
%USB\VID_04B5&PID_401B%=DrICEUsb.Dev, USB\VID_04B5&PID_401B

; For XP
[LAPISsemi.NTx86]
%USB\VID_070A&PID_401B%=DrICEUsb.Dev, USB\VID_070A&PID_401B
%USB\VID_04B5&PID_401B%=DrICEUsb.Dev, USB\VID_04B5&PID_401B

; For 7
[LAPISsemi.NTamd64]
%USB\VID_070A&PID_401B%=DrICEUsb.Dev, USB\VID_070A&PID_401B
%USB\VID_04B5&PID_401B%=DrICEUsb.Dev, USB\VID_04B5&PID_401B

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
; インストール先指定ブロック　nt ntx86 NTamd64 ntia64 環境名+CPU
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[DrICEUsb.Dev.NTx86]
CopyFiles = DrICEUsb.Files.Ext

[DrICEUsb.Dev.NTamd64]
CopyFiles = DrICEUsb.Files.Ext

[DrICEUsb.Files.Ext]
DrICEUsb.sys

;　サービスブロック
[DrICEUsb.Dev.NTx86.Services]
AddService = DrICEUsb,0x00000002,DrICEUsb.AddService

[DrICEUsb.Dev.NTamd64.Services]
AddService = DrICEUsb,0x00000002,DrICEUsb.AddService

[DrICEUsb.AddService] 
ServiceType = %SERVICE_KERNEL_DRIVER% 
StartType = %SERVICE_DEMAND_START% 
ErrorControl = %SERVICE_ERROR_NORMAL% 
ServiceBinary  = %10%\System32\Drivers\DrICEUsb.sys
AddReg         = DrICEUsb_AddReg
LoadOrderGroup = Base


[DrICEUsb_AddReg]
HKR,,DevLoader,,*ntkern
HKR,,NTMPDriver,,DrICEUsb.sys
HKR,"Parameters","MaximumTransferSize",0x10001,4096
HKR,"Parameters","DebugLevel",0x10001,2
;HKR,"Parameters","BulkUsbEnable",0x10001,1

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
; ソース指定ブロック　x86 amd64 ia64 CPU
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[SourceDisksNames.x86]
1=%Desc_x860%,,,\i386

[SourceDisksFiles.x86]
DrICEUsb.sys = 1;


[SourceDisksNames.amd64]
1=%Desc_x860%,,,\amd64

[SourceDisksFiles.amd64]
DrICEUsb.sys = 1;


;[DrICEUsb_EventLog_Inst] 
;AddReg = DrICEUsb_EventLog_Inst.AddReg 
 
;[DrICEUsb_EventLog_Inst.AddReg] 
;HKR,,EventMessageFile,%REG_EXPAND_SZ%,"%%SystemRoot%%\System32\IoLogMsg.dll" 
;HKR,,TypesSupported,%REG_DWORD%,7

;-------------- WDF Coinstaller installation
[DestinationDirs]
CoInstaller_CopyFiles = 11

[DrICEUsb.Dev.NTx86.CoInstallers]
AddReg=CoInstaller_AddReg
CopyFiles=CoInstaller_CopyFiles

[DrICEUsb.Dev.NTamd64.CoInstallers]
AddReg=CoInstaller_AddReg
CopyFiles=CoInstaller_CopyFiles

[CoInstaller_CopyFiles]
WdfCoInstaller01009.dll

[SourceDisksFiles]
;WdfCoInstaller01009.dll ; make sure the number matches with SourceDisksNames
WdfCoInstaller01009.dll=1 ; make sure the number matches with SourceDisksNames

[CoInstaller_AddReg]
HKR,,CoInstallers32,0x00010000, "WdfCoInstaller01009.dll,WdfCoInstaller"


[DrICEUsb.Dev.NTx86.Wdf]
KmdfService = DrICEUsb, DrICEUsb_wdfsect

[DrICEUsb.Dev.NTamd64.Wdf]
KmdfService = DrICEUsb, DrICEUsb_wdfsect

[DrICEUsb_wdfsect]
KmdfLibraryVersion = 1.9

;---------------------------------------------------------------;

[Strings] 

;  *******Localizable Strings******* 
LAPISsemi= "LAPIS Semiconductor Co., Ltd." 
MfgName = "LAPIS Semiconductor Co., Ltd."
Desc_x860= "DrICE" 

;ClassName       = "USB Device"
ClassName       = "USB"
USB\VID_070A&PID_401B = "LAPIS Semiconductor DrICE"
USB\VID_04B5&PID_401B = "LAPIS Semiconductor DrICE"

;  *******Non Localizable Strings******* 

SERVICE_BOOT_START = 0x0 
SERVICE_SYSTEM_START = 0x1 
SERVICE_AUTO_START = 0x2 
SERVICE_DEMAND_START = 0x3 
SERVICE_DISABLED = 0x4 

SERVICE_KERNEL_DRIVER = 0x1 
SERVICE_ERROR_IGNORE = 0x0 
SERVICE_ERROR_NORMAL = 0x1 
SERVICE_ERROR_SEVERE = 0x2 
SERVICE_ERROR_CRITICAL = 0x3 

REG_EXPAND_SZ = 0x00020000 
REG_DWORD = 0x00010001 
